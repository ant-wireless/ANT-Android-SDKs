<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.dsi.ant.sample.acquirechannels"
    android:versionCode="010300"
    android:versionName="01.03.00">
    <uses-sdk />

    <application
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme"
        android:allowBackup="true"
    >

        <activity
            android:name="com.dsi.ant.sample.acquirechannels.ChannelList"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <service android:name="com.dsi.ant.sample.acquirechannels.ChannelService" />
    </application>